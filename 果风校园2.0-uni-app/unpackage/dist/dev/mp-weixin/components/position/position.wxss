
.background{
	background-color: #FFFFFF;
	margin: 5%;
	width: 90%; height: 100rpx;
	margin-bottom: 180rpx;
	border-radius: 20rpx;
	display: flex;
	align-items: center;
}
.position_img{
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	left: 8%;
}
.position_user text{
	font-style: normal;
	font-weight: normal;
	font-size: 24rpx;
	line-height: 30rpx;
	color: #979797;
}
.position_text{
	font-style: normal;
	font-weight: normal;
	font-size: 28rpx;
	line-height: 36rpx;
	/* identical to box height */
	position: absolute;
	left: 18%;
	color: #000000;
}
.arrow3 {
	width: 30rpx;
	height: 30rpx;
	position: absolute;
	right: 8%;
}

